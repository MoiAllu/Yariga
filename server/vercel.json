{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@now/node"
      }
    ],
    "routes": [
      { "src": "/api/v1/users/(.*)", "dest": "/api/v1/users/$1" },
      { "src": "/api/v1/properties/(.*)", "dest": "/api/v1/properties/$1" },
      { "src": "/api/v1/posts/(.*)", "dest": "/api/v1/posts/$1" },
      { "src": "/(.*)", "dest": "/$1" }
    ]
  }