{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@now/node"
      }
    ],
    "rewrites": [
        { "source": "/api/v1/users/(.*)", "destination": "/api/v1/users/$1" },
        { "source": "/api/v1/properties/(.*)", "destination": "/api/v1/properties/$1" },
        { "source": "/(.*)", "destination": "/$1" }
      ]
  }